{
    "name": "gatsby-remark-embed-url",
    "description": "Gatsby plugin to embed script output",
    "version": "0.2.0",
    "author": "Nicu Reut <contact@nicu.dev>",
    "repository": {
        "type": "git",
        "url": "https://github.com/ecyshor/gatsby-remark-embed-url.git"
    },
    "dependencies": {
        "@babel/runtime": "^7.17.2",
        "node-fetch": "^3.2.0",
        "unist-util-visit": "^4.1.0",
        "mdast-util-to-string": "^3.1.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.17.6",
        "@babel/core": "^7.17.5",
        "babel-preset-gatsby-package": "^2.8.0",
        "cross-env": "^7.0.3"
    },
    "main": "index.js",
    "peerDependencies": {
        "gatsby": "^4.8.1"
    },
    "scripts": {
        "build": "babel src --out-dir . --ignore \"**/__tests__\"",
        "prepare": "cross-env NODE_ENV=production npm run build",
        "watch": "babel -w src --out-dir . --ignore \"**/__tests__\""
    },
    "engines": {
        "node": ">=10.13.0"
    },
    "keywords": [
        "gatsby",
        "gatsby-plugin",
        "gatsby-remark",
        "gatsby-embed-script",
        "gatsby-embed-gist",
        "gatsby-embed-url"
    ],
    "license": "MIT"
}
